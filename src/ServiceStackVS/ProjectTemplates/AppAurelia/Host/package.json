    "gulp-msbuild": "^0.4.7",
    "gulp-msdeploy": "^0.0.4",
    "gulp-nuget-pack": "^0.0.7",
    "gulp-nuget-restore": "^0.5.0",
    "gulp-util": "^3.0.7",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "raw-loader": "^0.5.1",
    "requirejs": "^2.3.2",
